{
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "none",
	"bracketSpacing": true,
	"arrowParens": "always",
	"printWidth": 80,
	"overrides": [
		{
			"files": "*.component.html",
			"options": {
				"parser": "angular"
			}
		}
	]
}
