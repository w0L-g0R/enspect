{
  "name": "enspect",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:pwa": "ng build && cd dist/enspect && npx pwa-asset-generator assets/enspect_icon.png icons --maskable false",
    "build:ghpages": "ng build --output-path gh_pages/enspect/docs --base-href /enspect/ && cd gh_pages/enspect/docs && npx pwa-asset-generator assets/enspect_icon.png icons --maskable false",
    "pwa": "cd dist/enspect && http-server -o",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:check": "eslint . --ext js,ts,json",
    "lint:fix": "eslint . --ext js,ts,json --quiet --fix",
    "prettier:check": "prettier --config .prettierrc --check 'src/**/*.{ts,css,html}'",
    "prettier:fix": "prettier --config .prettierrc --write 'src/**/*.{ts,css,html}'",
    "pre-commit": "npm run lint:fix && pretty-quick --staged && npm run test",
    "lint": "ng lint",
    "deploy:gcloud": "gsutil -m rsync -d -r dist/enspect gs://www.enspect.at"
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.4",
    "@angular/animations": "~12.2.0",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "^12.2.16",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "^12.2.16",
    "@apollo/client": "^3.5.10",
    "ajv": "^6.12.6",
    "apollo-angular": "^3.0.0",
    "echarts": "^5.3.0",
    "graphql": "^16.3.0",
    "ngx-echarts": "^8.0.1",
    "ngx-smart-modal": "^7.4.1",
    "ngx-typed-js": "^2.1.1",
    "pwa-asset-generator": "^6.0.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.10",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/video.js": "^7.3.28",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "angular-svg": "^2.0.8",
    "angular-svg-icon": "^13.0.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "include-media": "^1.4.10",
    "jasmine-core": "~3.8.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lottie-interactive": "^1.3.2",
    "lottie-web": "^5.8.1",
    "ngx-lottie": "^8.0.1",
    "ngx-page-scroll": "^7.0.6",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "typescript": "~4.3.5",
    "video.js": "^7.17.0"
  }
}
